Application Swan;

MainModule Swan swan
{
	nodeCoord = X;
	time = t;
	timeStep = Î´t;
	iterationMax = maxIter;
	timeMax = stopTime;
}

VtkOutput
{
	periodReferenceVariable = swan.n;
	outputVariables = swan.H as "hauteur", 
		swan.Dij as "profondeur";
}

