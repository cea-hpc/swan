{
	outputPath = "/swan/src-gen-cpp/stl-thread";
	N_CXX_COMPILER ="/usr/bin/g++";
}

Arcane
{
	outputPath = "/swan/src-gen-arcane";
	extension BathyLib providedBy BathyLibCpp;
	CMAKE_CXX_COMPILER = "/usr/bin/g++";
	Arcane_ROOT = "$ENV{HOME}/arcane/install";
}