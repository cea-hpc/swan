Application SwanGeo;

MainModule SwanGeo swanGeo
{
	nodeCoord = X;
	time = t;
	timeStep = Î´t;
	iterationMax = maxIter;
	timeMax = stopTime;
}


VtkOutput
{
	periodReferenceVariable = swanGeo.n;
	outputVariables = swanGeo.H as "hauteur", 
		swanGeo.Hplot as "plot";
}

